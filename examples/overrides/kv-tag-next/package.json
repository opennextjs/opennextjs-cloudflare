	"dependencies": {
		"react": "catalog:e2e",
		"react-dom": "catalog:e2e",
		"next": "catalog:e2e"
	},
	"devDependencies": {
		"@opennextjs/cloudflare": "workspace:*",
		"@playwright/test": "catalog:",
		"@types/node": "catalog:",
		"@types/react": "catalog:e2e",
		"@types/react-dom": "catalog:e2e",
		"typescript": "catalog:",
		"wrangler": "catalog:"
	}
}
