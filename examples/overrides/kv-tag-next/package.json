{
	"name": "kv-tag-next",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"build:worker": "pnpm opennextjs-cloudflare build --skipWranglerConfigCheck",
