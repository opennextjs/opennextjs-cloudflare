{
  "name": "r2-incremental-cache",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "d1:clean": "wrangler d1 execute NEXT_CACHE_D1 --command \"DROP TABLE IF EXISTS tags; DROP TABLE IF EXISTS revalidations\"",
    "build:worker": "pnpm d1:clean && pnpm opennextjs-cloudflare build",
    "preview:worker": "pnpm opennextjs-cloudflare preview",
    "preview": "pnpm build:worker && pnpm preview:worker",
    "e2e": "playwright test -c e2e/playwright.config.ts"
  },
  "dependencies": {
    "react": "catalog:e2e",
    "react-dom": "catalog:e2e",
    "next": "catalog:e2e"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "workspace:*",
    "@playwright/test": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:e2e",
    "@types/react-dom": "catalog:e2e",
    "typescript": "catalog:",
    "wrangler": "catalog:"
  }
}
